{
    "name": "leo-learning/moodle-logstore-elasticsearch",
    "description": "Sends Moodle events as xAPI statements to Elasticsearch using library classes from the xAPI logstore plugin",
    "homepage": "https://github.com/leo-learning/moodle-logstore-elasticsearch",
    "autoload": {
        "psr-4": {
            "XREmitter\\": "../xapi/lib/emitter/src/",
            "LogExpander\\": "../xapi/lib/expander/src/",
            "MXTranslator\\": "../xapi/lib/translator/src/"
        }
    },
    "require": {
        "pimple/pimple": "3.0.2",
        "elasticsearch/elasticsearch": "5.2.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.9"
    },
    "authors": [
        {
            "name": "Mike McGowan",
            "email": "mike.mcgowan@leolearning.com"
        }
    ]
}
